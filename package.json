{
	"name": "@jodev/svelte-toast",
	"version": "1.0.0",
	"exports": {
		".": "./src/lib/index.ts"
	},
	"keywords": [
		"svelte",
		"notifications",
		"toast",
		"snackbar"
	],
	"private": false,
	"svelte": "index.js",
	"license": "MIT",
	"author": {
		"email": "joseephnv@gmail.com",
		"name": "Joseph",
		"url": "https://github.com/jodev7"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/jodev7/svelte-toast"
	},
	"bugs": {
		"url": "https://github.com/jodev7/svelte-toast/issues"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"publish": "npm publish --access public",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "1.25.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/package": "^1.0.0-next.6",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.19",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svelte-writable-derived": "^2.1.5",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module",
	"dependencies": {
		"svelte-french-toast": "^1.0.3"
	}
}
